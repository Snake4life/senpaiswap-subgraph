{
  "name": "senpaiswap-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/Snake4life/senpaiswap-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "prepare:rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "create:subgraph:rinkeby": "graph create --node https://graph.rinkeby.boba.network:8020 boba/Senpai",
    "deploy:subgraph:rinkeby": "graph deploy boba/Senpai --ipfs https://graph.rinkeby.boba.network:5001 --node https://graph.rinkeby.boba.network:8020",
    "create:subgraph:mainnet": "graph create --node https://graph.mainnet.boba.network:8020 boba/Senpai",
    "deploy:subgraph:mainnet": "graph deploy boba/Senpai --ipfs https://graph.mainnet.boba.network:5001 --node https://graph.mainnet.boba.network:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}